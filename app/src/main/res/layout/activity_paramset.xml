<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@mipmap/bg01"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@mipmap/dingbu"
            android:textSize="16sp"
            android:textColor="#ffffff"
            android:text="ROS Master 连接"
            />
        <Button
            android:id="@+id/bt_paramset_back"
            android:layout_width="11dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:padding="0dp"
            android:background="@drawable/selector_back_top"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="10dp"
            android:clickable="true"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_pramaset_pvalue1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="P值"
            android:textColor="#ffffff"
            android:layout_alignParentTop="true"
            android:layout_marginTop="20dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="16dp"
            />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/et_paramset_pvalue1"
            android:textColor="#ffffff"
            android:textCursorDrawable="@null"
            android:background="#00000000"
            android:textSize="16sp"
            android:layout_below="@id/tv_pramaset_pvalue1"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            />
        <Button
            android:id="@+id/bt_paramset_pvaluedelete1"
            android:layout_width="11.5dp"
            android:layout_height="11.5dp"
            android:background="@drawable/selector_delete"
            android:layout_alignRight="@id/et_paramset_pvalue1"
            android:layout_alignTop="@id/et_paramset_pvalue1"
            />
        <ImageView
            android:id="@+id/iv_paramset_pvalue1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@mipmap/xian"
            android:layout_below="@id/et_paramset_pvalue1"
            android:layout_marginTop="11dp"
            android:scaleType="fitXY"
            android:layout_alignLeft="@id/et_paramset_pvalue1"
            android:layout_alignRight="@id/et_paramset_pvalue1"
            />
        <TextView
            android:id="@+id/tv_paramset_pvalue2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="P值"
            android:textColor="#ffffff"
            android:layout_below="@id/iv_paramset_pvalue1"
            android:layout_marginTop="32dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="16dp"
            />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/et_paramset_pvalue2"
            android:textColor="#ffffff"
            android:textCursorDrawable="@null"
            android:background="#00000000"
            android:textSize="16sp"
            android:layout_below="@id/tv_paramset_pvalue2"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            />
        <Button
            android:id="@+id/bt_paramset_pvaluedelete2"
            android:layout_width="11.5dp"
            android:layout_height="11.5dp"
            android:background="@drawable/selector_delete"
            android:layout_alignRight="@id/et_paramset_pvalue2"
            android:layout_alignTop="@id/et_paramset_pvalue2"
            />
        <ImageView
            android:id="@+id/iv_paramset_pvalue2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@mipmap/xian"
            android:layout_below="@id/et_paramset_pvalue2"
            android:layout_marginTop="11dp"
            android:scaleType="fitXY"
            android:layout_alignLeft="@id/et_paramset_pvalue2"
            android:layout_alignRight="@id/et_paramset_pvalue2"
            />
        <TextView
            android:id="@+id/tv_pvalue3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="P值"
            android:textColor="#ffffff"
            android:layout_below="@id/iv_paramset_pvalue2"
            android:layout_marginTop="32dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="16dp"
            />
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/et_pvalue3"
            android:textColor="#ffffff"
            android:textCursorDrawable="@null"
            android:background="#00000000"
            android:textSize="16sp"
            android:layout_below="@id/tv_pvalue3"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            />
        <Button
            android:id="@+id/bt_paramset_pvaluedelete3"
            android:layout_width="11.5dp"
            android:layout_height="11.5dp"
            android:background="@drawable/selector_delete"
            android:layout_alignRight="@id/et_pvalue3"
            android:layout_alignTop="@id/et_pvalue3"
            />
        <ImageView
            android:id="@+id/iv_paramset_pvalue3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@mipmap/xian"
            android:layout_below="@id/et_pvalue3"
            android:layout_marginTop="11dp"
            android:scaleType="fitXY"
            android:layout_alignLeft="@id/et_pvalue3"
            android:layout_alignRight="@id/et_pvalue3"
            />

        <Button
            android:layout_width="327dp"
            android:layout_height="40dp"
            android:id="@+id/bt_paramset_confirm"
            android:background="@drawable/selector_set_confirm"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="40dp"

            />



    </RelativeLayout>

</LinearLayout>